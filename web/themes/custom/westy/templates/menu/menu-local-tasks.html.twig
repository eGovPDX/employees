{#
/**
 * @file
 * Template primary and secondary local tasks.
 */
#}
{% if primary %}
  <h2 class="visually-hidden">{{ 'Primary tabs'|t }}</h2>
  <ul class="nav nav-pills bg-light" style="--bs-nav-link-padding-y: 0.25rem;">{{ primary }}</ul>
{% endif %}

{% if secondary %}
  <h2 class="visually-hidden">{{ 'Secondary tabs'|t }}</h2>
  <ul class="nav nav-pills my-2">{{ secondary }}</ul>
{% endif %}

