{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 8 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "require": {
        "bower-asset/cropper": "^4.1",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.0",
        "drupal/admin_toolbar": "^2.0",
        "drupal/anonymous_login": "^1.1",
        "drupal/config_split": "^1.4",
        "drupal/console": "~1.0",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-recommended": "^8.8",
        "drupal/crop": "^2.0",
        "drupal/ctools": "^3.0",
        "drupal/diff": "^1.0",
        "drupal/embed": "^1.3",
        "drupal/entity_block": "^1.0@beta",
        "drupal/entity_browser": "^2.4",
        "drupal/entity_browser_block": "^1.1",
        "drupal/entity_embed": "^1.0",
        "drupal/facets": "^1.4",
        "drupal/fixed_block_content": "^1.0",
        "drupal/image_widget_crop": "^2.3",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/key": "^1.8",
        "drupal/language_switcher_extended": "^1.0@beta",
        "drupal/layout_builder_restrictions": "^2.5",
        "drupal/layout_builder_st": "^1.0@alpha",
        "drupal/layout_builder_styles": "^1.0@beta",
        "drupal/layout_library": "^1.0@beta",
        "drupal/libraries": "^3.0@alpha",
        "drupal/masquerade": "^2.0@beta",
        "drupal/metatag": "^1.11",
        "drupal/moderation_dashboard": "^1.0@beta",
        "drupal/moderation_sidebar": "^1.3",
        "drupal/openid_connect": "^1.0@beta",
        "drupal/panels": "^4.4",
        "drupal/pathauto": "^1.6",
        "drupal/radix": "^4.3",
        "drupal/redirect": "^1.5",
        "drupal/redirect_404": "^1.4",
        "drupal/search_api_autocomplete": "^1.2",
        "drupal/search_api_location": "^1.0@alpha",
        "drupal/search_api_logger": "^1.3",
        "drupal/search_api_page": "1.0.0-alpha12",
        "drupal/search_api_solr": "3.1 as 1.2",
        "drupal/simple_gmap": "^2.0",
        "drupal/simple_oauth": "^4.4",
        "drupal/time_field": "^1.12",
        "drupal/toc_js": "^1.6",
        "drupal/toc_js_per_node": "^1.6",
        "drupal/token": "^1.6",
        "drupal/transliterate_filenames": "^1.3",
        "drupal/video_embed_field": "^2.2",
        "drupal/views_bulk_operations": "^3.2",
        "drupal/workbench": "^1.1",
        "drupal/workbench_access": "^1.0@beta",
        "drupal/workbench_moderation": "^1.5",
        "drush/drush": "^10.0",
        "hirak/prestissimo": "^0.3.9",
        "oomphinc/composer-installers-extender": "^1.1",
        "toc/toc": "*",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "wikimedia/composer-merge-plugin": "^1.4"
    },
    "require-dev": {
        "drupal-composer/drupal-security-advisories": "8.x-dev",
        "roave/security-advisories": "dev-master"
    },
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "1": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "2": {
             "type": "package",
             "package": {
                 "name": "toc/toc",
                 "version": "v0.3.2",
                 "type": "drupal-library",
                 "dist": {
                     "url": "https://raw.githubusercontent.com/jgallen23/toc/greenkeeper/update-all/dist/toc.min.js",
                     "type": "file"
                 }
             }
         }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library","type:bower-asset","type:npm-asset"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "composer-exit-on-patch-failure": true,
        "enable-patching": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/libraries": {
                "2882709 - Misnamed interface causes \"Fatal error: Cannot declare interface Drupal\\libraries\\ExternalLibrary\\Utility\\LibraryAccessorIdInterface\"": "https://www.drupal.org/files/issues/2882709-2-interface-libraryidaccessorinterface-misnamed.patch"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {}
}
