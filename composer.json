{
    "name": "acquia/lightning-project",
    "type": "project",
    "description": "Project template for Drupal 8 sites built with the Lightning distribution.",
    "license": "GPL-2.0-or-later",
    "require": {
        "acquia/lightning": "^4.0",
        "bower-asset/cropper": "^4.1",
        "bower-asset/dropzone": "^5.5",
        "bower-asset/slick-carousel": "^1.8",
        "cweagans/composer-patches": "^1.6.0",
        "drupal-composer/drupal-scaffold": "^2.0.0",
        "drupal/admin_toolbar": "^1.27",
        "drupal/anonymous_login": "^1.1",
        "drupal/bootstrap_datepicker": "^1.0@beta",
        "drupal/config_split": "^1.4",
        "drupal/console": "~1.0",
        "drupal/consumers": "^1.10",
        "drupal/core": "^8.8",
        "drupal/crop": "^2.0",
        "drupal/diff": "^1.0",
        "drupal/embed": "^1.3",
        "drupal/entity_block": "^1.0@beta",
        "drupal/entity_browser": "^2.4",
        "drupal/entity_browser_block": "^1.1",
        "drupal/facets": "^1.4",
        "drupal/fixed_block_content": "^1.0",
        "drupal/image_widget_crop": "^2.2",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/key": "^1.8",
        "drupal/language_switcher_extended": "^1.0@beta",
        "drupal/layout_builder_restrictions": "^2.5",
        "drupal/layout_builder_st": "^1.0@alpha",
        "drupal/layout_builder_styles": "^1.0@beta",
        "drupal/layout_library": "^1.0@beta",
        "drupal/libraries": "^3.0@alpha",
        "drupal/lightning_media": "^3.16",
        "drupal/media": "1.x-dev",
        "drupal/media_entity_image": "^1.3",
        "drupal/metatag": "^1.11",
        "drupal/moderation_dashboard": "^1.0@beta",
        "drupal/moderation_sidebar": "^1.3",
        "drupal/openid_connect": "^1.0@beta",
        "drupal/panels": "^4.4",
        "drupal/pathauto": "^1.6",
        "drupal/permissions_by_term": "^2.10",
        "drupal/radix": "^4.3",
        "drupal/redirect": "^1.5",
        "drupal/redirect_404": "^1.4",
        "drupal/search_api": "^1.15",
        "drupal/search_api_autocomplete": "^1.2",
        "drupal/search_api_solr": "3.1 as 1.2",
        "drupal/simple_gmap": "^2.0",
        "drupal/time_field": "^1.12",
        "drupal/token": "^1.6",
        "drupal/transliterate_filenames": "^1.3",
        "drupal/video_embed_field": "^2.2",
        "drupal/views_bulk_operations": "^3.2",
        "drush/drush": "^9.0",
        "hirak/prestissimo": "^0.3.9",
        "drupal/openapi": "^1.0-beta6",
        "drupal/openapi_ui_redoc": "^1.0",
        "drupal/openapi_ui_swagger": "^1.0",
        "drupal/simple_oauth": "^3.16",
        "drupal/contact_storage": "^1.0.0-beta10",
        "drupal/views_infinite_scroll": "^1.6",
        "drupal/autosave_form": "^1.0",
        "drupal/conflict": "2.0-alpha1",
        "drupal/dropzonejs": "2.0.0-alpha4",
        "drupal/entity_embed": "1.0",
        "drupal/media_entity_instagram": "^2.0",
        "drupal/media_entity_twitter": "^2.1",
        "drupal/slick_entityreference": "^2.0",
        "vardot/blazy": "^1.8",
        "drupal/bg_image_formatter": "^1.10",
        "drupal/ctools": "^3.0",
        "drupal/panelizer": "^4.1 || ^5.0",
        "drupal/acquia_connector": "*",
        "phpdocumentor/reflection-docblock": "^3.0 || ^4.0",
        "oomphinc/composer-installers-extender": "^1.1"
    },
    "require-dev": {
        "drupal-composer/drupal-security-advisories": "8.x-dev",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "enable-patching": true,
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "web/drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "patches": {}
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "assets": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "dev": {
            "type": "github",
            "url": "https://github.com/acquia/lightning-dev",
            "no-api": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {}
}
