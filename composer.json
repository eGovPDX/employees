        "drupal/structure_sync": "^2.0",
        "drupal/subpathauto": "^1.1",
        "drupal/toc_js": "^3.2",
        "drupal/token": "^1.6",
        "drupal/toolbar_menu": "^3.0",
        "drupal/tour": "^2.0",
