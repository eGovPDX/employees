    "jest": "^27.5.1",
    "puppeteer": "^13.5.2",
    "ws": "^8.5.0"
  },
  "resolutions": {
    "browserslist": "^4.16.5",
    "set-value": "^4.0.1",
    "ansi-regex": "^5.0.1"
  }
}
