{
  "name": "test-suite",
  "version": "1.0.0",
  "description": "Test suite for employees.portland.gov",
  "scripts": {
    "jest-test": "jest --config ./jest.config.js",
    "jest-full": "jest --config ./jest.full-regression.config.js",
    "jest-sandbox": "jest --config ./jest.sandbox.config.js",
    "single-test": "percy exec -- jest --config ./jest.config.js",
    "single-test:watch": "percy exec -- jest --config ./jest.config.js --watch",
    "all-tests": "percy exec -- jest --runInBand --config ./jest.config.js"
  },
  "devDependencies": {
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0"
  },
  "author": "City of Portland",
  "dependencies": {
    "@percy/cli": "^1.29.4",
    "@percy/puppeteer": "^2.0.2",
    "ansi-regex": "^6.1.0",
    "browserslist": "^4.24.0",
    "jest": "^29.7.0",
    "minimist": ">=1.2.6",
    "puppeteer": "^23.5.0",
    "ws": "^8.18.0"
  },
  "resolutions": {
    "browserslist": "^4.16.5",
    "set-value": "^4.0.1",
    "ansi-regex": "^5.0.1",
    "minimist": ">=1.2.6",
    "json5": "^2.2.2"
  }
}
